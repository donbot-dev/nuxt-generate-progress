  "bugs": {
    "url": "https://github.com/robot-mafia/nuxt-generate-progress/issues"
  },
  "homepage": "https://github.com/robot-mafia/nuxt-generate-progress#readme",
  "devDependencies": {
    "@nuxt/types": "^2.14.1",
    "consola": "^2.15.0",
    "eslint": "^7.6.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.0",
    "typescript": "^4.0.2"
  },
  "dependencies": {
